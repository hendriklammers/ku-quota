{
  "name": "ku-quota",
  "version": "1.0.0",
  "description": "Shows your remaining internet data for Kasetsart University network",
  "repository": "github:hendriklammers/ku-quota",
  "main": "dist/index.js",
  "types": "dist",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "start": "nodemon --watch src/**/*.ts --exec ts-node src/index.ts"
  },
  "keywords": [
    "node",
    "typescript",
    "Kasetsart University"
  ],
  "author": "Hendrik Lammers <lammers@gmail.com> (http://www.hendriklammers.com)",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.0.0-rc.2"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.11",
    "@types/node": "^11.11.6",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.4000"
  },
  "bin": {
    "ku-quota": "dist/index.js"
  },
  "files": [
    "dist/**/*"
  ]
}
